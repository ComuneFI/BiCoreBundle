<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <title>{{ parameter('bi_core.appname')}} Login</title>
        <link rel="stylesheet" href="{{ asset('bundles/bicore/css/bootstrap-italia.min.css') }}" >
        <link rel="stylesheet" href="{{ asset('bundles/bicore/css/login.css') }}" >  
        <script type="text/javascript">
            window.__PUBLIC_PATH__ = '{{ asset('bundles/bicore/fonts') }}';
            var baseUrl = '{{app.request.baseUrl}}';
        </script>
    </head>
    <body>
        {% for type, messages in app.session.flashBag.all %}
            {% for message in messages %}
                <div class="{{ type }}">
                    {{ message|trans({}, 'FOSUserBundle') }}
                </div>
            {% endfor %}
        {% endfor %}
    {% block fos_user_content %}{% endblock %}
{% block javascripts %}{% endblock %}
<script type="text/javascript" src="{{ asset('bundles/bicore/js/bootstrap-italia.bundle.min.js') }}" ></script>
</body>
</html>
