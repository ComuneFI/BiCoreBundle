{% extends '@WebProfiler/Profiler/:layout.html.twig' %}

{% block toolbar %}
    {# contenuto della barra di debug del web #}
    {% set icon %}
        <div id="img_wdt_db">
            <img width="20" height="28" alt="Database" src="data:image/png;base64,"/>
        </div>
        <div id="img_wdt_db_wait" style="display: none">
            <img width="20" height="28" alt="Database" src="{{ asset('bundles/bicore/images/waitpage.gif') }}"/>
        </div>
    {% endset %}
    {% set text %}

        <div class="sf-toolbar-info-piece">
            <b>Driver</b><span class="sf-toolbar-status">{{ collector.DatabaseDriver }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Host</b><span class="sf-toolbar-status sf-toolbar-status-green">{{ collector.DatabaseHost }}{{  collector.DatabasePort ? ':' ~ collector.DatabasePort : '' }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Database</b><span class="sf-toolbar-status sf-toolbar-status-yellow">{{ collector.DatabaseName }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>User</b><span class="sf-toolbar-status">{{ collector.DatabaseUser }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Password</b><span>{{ collector.DatabasePassword[:2] ~ '*****' }}</span>
        </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block menu %}
    {# contenuto del men√π #}
    <span class="label">
        <span class="icon">
            <img width="20" height="28" alt="Database" src="data:image/png;base64,"/>
        </span>
        <strong>Database</strong>
    </span>

{% endblock %}

{% block panel %}
    {# contenuto del pannello #}
    <div class="sf-toolbar-info-piece">
        <table style="width: 30%">
            <tr>
                <td>Driver</td><td><b>{{ collector.DatabaseDriver }}</b></td>
            </tr>
            <tr>
                <td>Host</td><td><b>{{ collector.DatabaseHost }}{{  collector.DatabasePort ? ':' ~ collector.DatabasePort : '' }}</b></td>
            </tr>
            <tr>
                <td>Database</td><td><b>{{ collector.DatabaseName }}</b></td>
            </tr>
            <tr>
                <td>User</td><td>{{ collector.DatabaseUser }}</td>
            </tr>
            <tr>
                <td>Password</td><td><i>{{ collector.DatabasePassword[:2] ~ '*****' }}</i></td>
            </tr>
        </table>

    </div>
{% endblock %}
