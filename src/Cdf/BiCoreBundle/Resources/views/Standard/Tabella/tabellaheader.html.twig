<thead>
    <tr>
        <th scope="col" id="bititletable{{parametri.nomecontroller|getparametrotabella}}" class="text-center table-secondary">
            {{parametri.titolotabella|getparametrotabella}}
        </th>
    </tr>
    <tr class="filters">
        <th scope="col" class="text-center">
            <input type="checkbox" class="{% if (parametri.multiselezione|getparametrotabella == false) %}sr-only{% else %}bitabellacheckboxselectall{% endif %}">
        </th>
        {% for colonnatabella in parametri.parametritabella %}
            {% if (colonnatabella.escluso == false) %}
                <th scope="col" class="{{colonnatabella.larghezza|larghezzacolonna}}">
                    <input type="text" class="form-control {{colonnatabella.association?"":'colonnatabellafiltro'}}" data-nomecampo="{{colonnatabella.nomecampo}}" data-tipocampo="{{colonnatabella.tipocampo}}" {{colonnatabella.decodifiche is defined?'data-decodifiche='~colonnatabella.decodifiche|json_encode():""}} data-nomecontroller="{{parametri.nomecontroller|getparametrotabella}}" placeholder="{{colonnatabella.etichetta}}" disabled>
                </th>
            {% endif %}
        {% endfor%}
        <th scope="col" class="col-auto"></th>
    </tr>
</thead>
